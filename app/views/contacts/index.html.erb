<table class="table">
  <tr>
    <th>Full Name</th>
    <th>Phone Number</th>
    <th>Updated At:</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.full_name %></td>
      <td><%= contact.phone_number_plus_jp_prefix %></td>
      <td><%= contact.friendly_time %></td>
    </tr>
  <% end %>

</table>